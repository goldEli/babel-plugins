	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"outputCapture": "std",
			"name": "index",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/index.js",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"preLaunchTask": "tsc"
		},
		{
			"type": "node",
			"request": "launch",
			"outputCapture": "std",
			"name": "console",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/packages/plugin-console/src/index.js",
		}
	]
}